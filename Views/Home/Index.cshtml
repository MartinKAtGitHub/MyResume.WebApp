@model UserSearchViewModel




    <div id="HomePageLogoContainer">


    </div>

    <div>
        <form asp-controller="Home" asp-action="Index" method="get">
            <div class="form-group">
                <input class="form-control " type="text" placeholder="Search" asp-for="SearchString">
                @*<button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>*@
            </div>
        </form>
    </div>

    @if (Model.UsersResult != null)
    {
        <ul>
            @foreach (var User in Model.UsersResult)
            {
                <li>
                    <a asp-controller="Home" asp-action="UserResume" asp-route-id="@User.Id">@User.UserName</a>
                </li>
            }
        </ul>
    }


