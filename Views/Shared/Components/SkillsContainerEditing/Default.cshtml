@model List<Skill>

@if (Model.Any())
{
    for (int i = 0; i < Model.Count; i++)
    {
            @await Component.InvokeAsync("SkillEditDisplay", new { skill = Model[i] })
    }
    <div class="text-danger" asp-validation-summary="All"></div>
}
else
{
    <div> You have not created any skills</div>
}